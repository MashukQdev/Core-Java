package com.main;

/**
 Simple program to demonstrate use of if / else statement
 Practical 2 from core java practicals
 @author Mashuk Patel
 */
public class IfElseStatement {

	public static void main(String[] args) {
		int age=18; 
		if(age >= 0 && age <= 18) {
			System.out.println("Age is between 0 and 18.");
		}
		else if(age >= 19 && age <= 35) {
			System.out.println("Age is between 19 and 35.");
		}
		else {
			System.out.println("Age is above 35.");
		}
	}
	
}
